              <ProjectItem
                Name="ScriptMain.vb"
                Encoding="UTF8"><![CDATA[Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime
<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute> _
<System.CLSCompliantAttribute(False)> _
Partial Public Class ScriptMain
	Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

	Enum ScriptResults
		Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
		Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
	End Enum
    Public Sub Main()
        Dim messageSubject As String = "Automatic Notification - General Services Fuel Pump Connection Status"
        Dim message As String = "General Services Fuel Pump Connection Status" & vbCrLf & vbCrLf & vbCrLf
        Dts.Variables("MessageSubject").Value = messageSubject
        Dim cnt As Integer
        '
        Dim con2 As New SqlClient.SqlConnection("data source=msb-db-prod; initial catalog=Fleet; integrated security=SSPI;")
        con2.Open()
        '
        Dim cmd2 As New SqlClient.SqlCommand("SELECT * FROM Pumps", con2)
        Dim dr2 As SqlClient.SqlDataReader = cmd2.ExecuteReader
        While dr2.Read
            ' Get number of pumps for next SQL.
            cnt = cnt + 1
        End While

        If cnt > 0 Then
            Dim con As New SqlClient.SqlConnection("data source=msb-db-prod; initial catalog=Fleet; integrated security=SSPI;")
            con.Open()        '
            ' This was originally designed to grab the LATEST records.  
            ' It did not always work so DELETE FROM PUMPIPSTATUS task was added to the project.
            ' Only the current Pings will ever be in the table.
            Dim cmd As New SqlClient.SqlCommand("SELECT TOP " & cnt.ToString & " " & _
                "Pump, CONVERT(CHAR(26),PingDate,120) as PingDate, IP, Status " & _
                "FROM  PumpIPstatus " & _
                " GROUP BY Pump, PingDate, IP,  Status " & _
                " ORDER BY 2,1", con)
            Dim dr As SqlClient.SqlDataReader = cmd.ExecuteReader
            message = "Ping results:" & vbCrLf
            message = message & "Date                      IP              Pump/Response Time" & vbCrLf
            message = message & "----------------------    --------------- ------------------" & vbCrLf
            While dr.Read
                message = message & dr.GetString(1).PadRight(21) & dr.GetString(2).PadRight(16) & _
                    dr.GetString(0).PadRight(16) & vbCrLf
                'System.Windows.Forms.MessageBox.Show(message)
            End While
            dr.Close()
            con.Close()
            message = message & vbCrLf & vbCrLf
            message = message & "Attention OWAM Fuel Interface Admins:" & vbCrLf
            message = message & "Any pumps appearing 'Offline' indicates a connection issue." & vbCrLf
            message = message & "Forward to helpdesk and include request for Network to check breakers." & vbCrLf
            message = message & "If problem persists consult SCI support for service at 800.831.0220 - support@sciww.com" & vbCrLf & vbCrLf
        Else
            message = message & vbCrLf & vbCrLf
            message = message & "Attention OWAM Fuel Interface Admins:" & vbCrLf
            message = message & "ERROR: There are no pumps listed in the Fleet.Pumps table." & vbCrLf
            message = message & "Populate the table then retry PingFuelPumps package." & vbCrLf
        End If
        dr2.Close()
        con2.Close()
        message = message & vbCrLf
        message = message & cnt.ToString & " pumps read."
        Dts.Variables("MessageContent").Value = message
        Dts.TaskResult = ScriptResults.Success
    End Sub
End Class]]></ProjectItem>
