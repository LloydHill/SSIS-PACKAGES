'Imports System.ThreadingModule


<Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute> _
<CLSCompliant(False)> _
Public Class ScriptMain
    Inherits UserComponent
    Public Overrides Sub Input0_ProcessInputRow(ByVal Row As Input0Buffer)
        Try
            '            Dim _WaitBetweenPings As New Threading.ManualResetEvent(False)
            'Pumps as of 3/2014:
            ' D1...172.18.18.41  CNG1.172.18.18.141
            ' D2...172.18.18.42  CNG2.172.18.18.142
            ' D3...172.18.18.43  CNG3.172.18.18.143
            ' D4...172.18.18.44  CNG4.172.18.18.144
            ' U5...172.18.18.45  CNG5.172.18.18.145
            ' U6...172.18.18.46  CNG6.172.18.18.146
            ' U7...172.18.18.48  CNG7.172.18.18.147
            ' U8...172.18.18.47  CNG8.172.18.18.148
            Dim MyPing As New System.Net.NetworkInformation.Ping
            Dim Myreply As System.Net.NetworkInformation.PingReply
            Me.Output0Buffer.AddRow()
            Try
                Myreply = MyPing.Send(Row.IP.ToString.ToString)
                Me.Output0Buffer.PingPump = Row.Pump.ToString & "Pinging..."
                Me.Output0Buffer.PingIP = Row.IP.ToString
                Me.Output0Buffer.PingDate = DateTime.Parse(DateTime.Now.ToString("yyyy/MM/dd HH:mm:ss"))
                Select Case Myreply.RoundtripTime
                    Case 0
                        Me.Output0Buffer.PingPump = Row.Pump.ToString & " Offline."
                        Me.Output0Buffer.PingStatus = False
                    Case Else
                        Me.Output0Buffer.PingPump = Row.Pump.ToString & " " & Myreply.RoundtripTime.ToString & "ms"
                        Me.Output0Buffer.PingStatus = True
                End Select
                '                _WaitBetweenPings.WaitOne()
                '                _WaitBetweenPings.Set()     ' Allow next thread to proceed.
            Catch ex As Exception
                Me.Output0Buffer.PingPump = Row.Pump.ToString & "There was an Error in ScriptMain of DataFlow Task in PingFuelPumps SSIS."
                Me.Output0Buffer.PingIP = Row.IP.ToString
                Me.Output0Buffer.PingDate = DateTime.Parse(DateTime.Now.ToString("yyyy/MM/dd HH:mm:ss"))
            End Try
        Catch ex As Exception

        End Try
    End Sub
End Class

